{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "leaderboard/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "leaderboard/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/leaderboard",
      "destination": "main.py"
    },
    {
      "source": "/leaderboard",
      "destination": "/leaderboard/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
